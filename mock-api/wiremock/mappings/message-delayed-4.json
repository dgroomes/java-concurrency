{
  "request": {
    "method": "GET",
    "urlPath": "/message",
    "queryParameters": {
      "delay": {
        "matches": "4"
      }
    }
  },
  "response": {
    "status": 200,
    "transformers": [
      "response-template"
    ],
    "bodyFileName": "hello-world-delayed.txt",
    "fixedDelayMilliseconds": 4000
  }
}